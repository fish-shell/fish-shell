#define LINK_SIZE		@PCRE2_LINK_SIZE@
#define HEAP_LIMIT              @PCRE2_HEAP_LIMIT@
#define MATCH_LIMIT		@PCRE2_MATCH_LIMIT@
#define MATCH_LIMIT_DEPTH	@PCRE2_MATCH_LIMIT_DEPTH@
